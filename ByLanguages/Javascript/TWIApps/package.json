{
  "name": "twiapps",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run TWIUniversalWeb:serve-ssr",
    "build:ssr": "ng build --prod && ng run TWIUniversalWeb:server:production && tsc --project TWIUniversalWeb",
    "serve:ssr": "node dist/server/main.js",
    "prerender": "ng run TWIUniversalWeb:prerender",
    "compile-TWIUniversalWeb": "tsc --project TWIUniversalWeb",
    "compile-ssr-TWIUniversalWeb": "npm run compile-TWIUniversalWeb && cp dist/index.html TWIUniversalWeb/dist",
    "prod:build:ssr": "ng build --prod --configuration=production && ng run TWIUniversalWeb:server:production",
    "prod:deploy-TWIUniversalWeb-build": "npm run prod:build:ssr && npm run compile-ssr-TWIUniversalWeb",
    "prod:deploy-TWIUniversalWeb": "npm run prod:deploy-TWIUniversalWeb-build && npm run prod:set-index && npm run prod:deploy-TWIUniversalWeb-app && npm run prod:deploy-TWIUniversalWeb-ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.6",
    "@angular/common": "~9.0.6",
    "@angular/compiler": "~9.0.6",
    "@angular/core": "~9.0.6",
    "@angular/forms": "~9.0.6",
    "@angular/platform-browser": "~9.0.6",
    "@angular/platform-browser-dynamic": "~9.0.6",
    "@angular/platform-server": "~9.0.6",
    "@angular/pwa": "^0.901.1",
    "@angular/router": "~9.0.6",
    "@angular/service-worker": "~9.0.6",
    "@angular/upgrade": "~9.0.6",
    "@nguniversal/express-engine": "~9.0.1",
    "angular-in-memory-web-api": "~0.9.0",
    "express": "^4.15.2",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.6",
    "@angular-devkit/build-ng-packagr": "~0.901.1",
    "@angular/cli": "~9.0.6",
    "@angular/compiler-cli": "~9.0.6",
    "@nguniversal/builders": "^9.0.2",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lodash": "^4.16.2",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.7.5"
  }
}
